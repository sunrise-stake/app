{
  "name": "@sunrisestake/client",
  "version": "0.1.17",
  "description": "Sunrise Stake Client",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist; tsc -p tsconfig.esm.json; tsc -p tsconfig.cjs.json && yarn typedoc",
    "lint": "eslint -c ../../.eslintrc.yml --ext .ts,.tsx src",
    "test": "mocha --loader ts-node/esm --extensions ts 'src/__tests__/**/*.test.ts'"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@metaplex-foundation/js": "^0.18.3",
    "@project-serum/borsh": "^0.2.5",
    "@solana/spl-stake-pool": "^1.1.8",
    "@solana/spl-token": "0.3.8",
    "@solana/web3.js": "^1.98.2",
    "@sunrisestake/impact-nft-client": "0.0.5-module.4",
    "@sunrisestake/marinade-ts-sdk": "^4.0.4-alpha.18",
    "bn.js": "^5.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "chai": "^4.3.10",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^6.6.3",
    "typedoc": "^0.28.7",
    "typescript": "^5.3.0"
  }
}
